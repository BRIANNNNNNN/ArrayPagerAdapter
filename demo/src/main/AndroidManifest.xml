<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.arraypargeradapter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.demo.arraypageradapter.TopActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.demo.arraypageradapter.FragmentStatePagerAdapterActivity"
            android:label="@string/label_fragment_state"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.demo.arraypageradapter.FragmentPagerAdapterActivity"
            android:label="@string/label_fragment"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.demo.arraypageradapter.ViewPagerAdapterActivity"
            android:label="@string/label_view_state"
            android:windowSoftInputMode="stateHidden" />
    </application>

</manifest>
